{% macro sample(name) %}
	<div>
        {{box()}}
        Sang : {{ name }}
    {{spaces()}}
        Nombre de tubes :
        {{ underscored(10) }}
    </div>
{% endmacro %}

{% macro underscored(size) %}
     <div style="width: {{size}}%" class="underscored"></div>
{% endmacro %}

{% macro box(class = "") %}
    <div class="box {{ class }}"></div>
{% endmacro %}

{% macro spaces() %}
    &nbsp;&nbsp;&nbsp;
{% endmacro %}